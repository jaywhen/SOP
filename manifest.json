{
    "manifest_version": 2,
    "name": "SOP",
    "homepage_url": "https://jaywhen.com",
    "description": "面向搜索引擎编程",
    "version": "0.0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
      },
    "permissions":[
        "clipboardWrite",
        "contextMenus", 
        "tabs"
    ]
}